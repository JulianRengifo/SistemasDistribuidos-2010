package services;

import java.io.DataOutputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.net.Socket;
import java.io.IOException;

public class Client{
	
	protected Socket s;
	
	private final int PORT = 1234;
	private final String HOST = "localhost";
	
	protected ObjectInputStream ois;
	protected Object OutputStream oos;

	public Client()  {}
		
		public void onClient(int client) throws ClassNotFoundExcepcion{
	
		
		try {
		ois = new ObjectInputSream(s.getInputStream());
		oos = new ObjectOutputSream (s.getOutputStream());
		
		Persona persona = new Persona (client, "julian", 23, "M");
		System.out.println(persona);
		
		oos.writeObject(persona);
		System.out.println(ois.readObject());
		
		//ois.colese();
		//oss.close();
		
	}catch(IOException e) {
		System.out.println(e.getMessage());
	}

	
	}
}
